.listWrap .mainView {
  height: 100vh;
  box-sizing: border-box;
}
.record_li {
  width: calc(100vw - 64rpx);
  padding: 32rpx 0;
  margin: 0 32rpx;
  display: flex;
  position: relative;
}
.record_li:after {
  content: '';
  height: 1px;
  width: 100%;
  position: absolute;
  bottom: 0;
  background: rgba(226, 226, 226, 0.5);
  transform: scaleY(0.5);
}
.record_li .record_img {
  flex-shrink: 0;
  width: 220rpx;
  height: 136rpx;
}
.record_li .right {
  margin-left: 24rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.record_li .right .record_tit {
  font-size: 30rpx;
  color: #191919;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width:450rpx;
}
.record_li .right .record_time {
  text-align: right;
  color: #b4b4b4;
  font-size: 22rpx;
}

.listWrap .mainView .delBox {
 text-align:center;
  margin-top: 48rpx;
  margin-bottom:48rpx;
}
.listWrap .mainView .delBox view{
  display: inline-block;
}

.listWrap .mainView .delBox .delImg {
  width: 33rpx;
  height: 33rpx;
  margin-right:10rpx;
}
.listWrap .mainView .delBox .delTxt {
  font-size: 28rpx;
  color: #666;
}
.listWrap .mainView .header {
  font-size: 24rpx;
  color: #b4b4b4;
  text-align: center;
  margin-top: 10rpx;
}
.listWrap .empBox {
  margin-top: 32vh;
  width: 100vw;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.listWrap .empBox .empImg {
  width: 325rpx;
  height: 154rpx;
}
.listWrap .empBox .empTxt {
  margin-top: 42rpx;
  font-size: 30rpx;
  color: #B4B4B4;
}
.listWrap .backTop {
  position: fixed;
  right: 50rpx;
  bottom: 120rpx;
  width: 140rpx;
  height: 140rpx;
}
