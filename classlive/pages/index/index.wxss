.tab-h {
	width: 100%;
	box-sizing: border-box;
	overflow: hidden;
  height:80rpx;
	white-space: nowrap;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 99;
	background: #fff;
  box-shadow:0px 10rpx 18rpx 0rpx rgba(104,104,104,0.05);
}

.tab-item {
  line-height:1;
  height:80rpx;
  position:relative;
	margin: 0 36rpx;
	display: inline-block;
	font-size: 28rpx;
  transition:all .3s ease;
}

.tab-item.active {
	font-size: 32rpx;
	font-weight: 600;
}

swiper {
	height: 424rpx
}

.tab-item:after{
	content: "";
	position:absolute;
  bottom:30rpx;
  left:50%;
  margin-left:-27.5rpx;
	height: 10rpx;
	width: 55rpx;
	background: url(https://www.vipboluo.com/filesinfo/首页_选中.png) no-repeat center/cover;
  opacity: 0;
  transform: scale(0);
  transition:all .3s ease;
}

.tab-item.active:after {
	opacity: 1;
  transform: scale(1);
  transition:all .3s ease;
}

